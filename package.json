{
  "name": "wishwander",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json",
    "sim:disruption": "node --loader ts-node/esm tools/simulate/disruption.ts",
    "sim:weather": "node --loader ts-node/esm tools/simulate/weather.ts",
    "test:e2e:web": "playwright test e2e/web",
    "test:e2e:mobile": "echo 'mobile e2e skipped'",
    "test:a11y": "node tools/axe.js",
    "server": "ts-node server/index.ts"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "/e2e/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(?:@react-native|react-native|@react-native-async-storage|@react-native/js-polyfills)/)"
    ]
  },
  "dependencies": {
    "@appandflow/react-native-google-autocomplete": "^2.1.0",
    "@expo/vector-icons": "^14.1.0",
    "@google/generative-ai": "^0.21.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.5",
    "date-fns": "^4.1.0",
    "expo": "~53.0.22",
    "expo-auth-session": "~6.2.1",
    "expo-background-fetch": "~13.1.6",
    "expo-background-task": "~0.2.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-random": "^14.0.1",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-task-manager": "~13.1.6",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.0.1",
    "json5": "^2.2.3",
    "moment": "^2.30.1",
    "nativewind": "^4.1.21",
    "expo-secure-store": "~14.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendar-picker": "^8.0.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-google-places-autocomplete": "^2.5.7",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-swiper": "^1.6.0",
    "react-native-web": "^0.20.0",
    "setimmediate": "^1.0.5",
    "stripe": "^18.5.0",
    "tailwindcss": "^3.4.14",
    "express": "^4.19.2",
    "ws": "^8.18.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@playwright/test": "^1.55.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-native-calendar-picker": "^8.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "detox": "^20.41.1",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.10",
    "@axe-core/playwright": "^4.10.2",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-swiper",
          "firebase",
          "moment"
        ]
      }
    }
  },
  "private": true
}
